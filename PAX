    <div class='main-content-area'>
      <b:section id='main' showaddelement='yes'>
        <b:widget id='HTML1' locked='false' title='' type='HTML'>
          <b:widget-settings>
            <b:widget-setting name='content'><![CDATA[<script type='text/javascript'>
//<![CDATA[
document.addEventListener('DOMContentLoaded', function() {
    const postBody = document.querySelector('.post-body.entry-content');
    const tabsContainer = document.getElementById('generated-tabs-container');
    const sourceData = document.querySelector('#post-body-content');

    if (!postBody || !tabsContainer || !sourceData) {
        return;
    }

    let rawContent = sourceData.innerHTML;
    let anyTabFound = false;
    let backgroundUrl = '';
    let tipoContenido = 'Episodio';

    const backgroundRegex = /<Fondo-Listas>\s*([\s\S]*?)\s*<\/Fondo-Listas>/i;
    const tipoRegex = /<Tipo-Contenido>\s*([\s\S]*?)\s*<\/Tipo-Contenido>/i;

    const backgroundMatch = backgroundRegex.exec(rawContent);
    const tipoMatch = tipoRegex.exec(rawContent);

    if (backgroundMatch) {
        backgroundUrl = backgroundMatch[1].trim();
        rawContent = rawContent.replace(backgroundMatch[0], '');
    }

    if (tipoMatch) {
        tipoContenido = tipoMatch[1].trim();
        rawContent = rawContent.replace(tipoMatch[0], '');
    }

    let textoBase = '';
    let tieneNumero = true;
    switch (tipoContenido.toLowerCase()) {
        case 'ova':
            textoBase = 'OVA';
            break;
        case 'ona':
            textoBase = 'ONA';
            break;
        case 'especial':
            textoBase = 'Especial';
            break;
        case 'pelicula':
            textoBase = 'Película';
            tieneNumero = false;
            break;
        case 'av':
            textoBase = 'Parte';
            tieneNumero = true;
            break;
        default:
            textoBase = 'Episodio';
            break;
    }

    const serverRegex = /<Enlaces-([^>]+)>\s*([\s\S]*?)\s*<\/Enlaces-[^>]+>/gi;
    let match;

    let tabButtonsHtml = '<div class="tab-buttons">';
    let tabContentsHtml = '';
    let firstTabActive = true;
    let finalContent = rawContent;

    const servers = [];
    while ((match = serverRegex.exec(rawContent)) !== null) {
        servers.push({
            name: match[1],
            content: match[2],
            fullMatch: match[0]
        });
    }

    let onlineLinks = [];
    let processedServers = [];

    servers.forEach(server => {
        const linksContent = server.content;
        const links = linksContent.split(/<br\s*\/?>|\n/).map(link => link.trim()).filter(link => link !== '');

        if (links.length > 0 && links.length < 3) {
            links.forEach((link, linkIndex) => {
                onlineLinks.push({
                    serverName: server.name.toUpperCase(),
                    linkUrl: link,
                    totalLinks: links.length,
                    linkIndex: linkIndex + 1
                });
            });
            finalContent = finalContent.replace(server.fullMatch, '');
            processedServers.push(server.name);
        }
    });

    if (onlineLinks.length > 0) {
        anyTabFound = true;
        const isActive = firstTabActive ? ' active' : '';
        const tabId = 'online';

        tabButtonsHtml += `<button class="tab-button${isActive}" data-tab="${tabId}"><i class="fa-regular fa-circle-play"></i> ONLINE</button>`;
        tabContentsHtml += `<div id="tab-${tabId}" class="tab-content${isActive}"><ul>`;

        const backgroundStyle = backgroundUrl ? `background-image: url('${backgroundUrl}');` : '';

        onlineLinks.forEach((item) => {
            let episodeTitleHtml = '';
            if (tipoContenido.toLowerCase() === 'av' && item.totalLinks === 1) {
                // No hay leyenda de tipo
            } else if (tipoContenido.toLowerCase() === 'pelicula') {
                episodeTitleHtml = `<span class="episode-title"><i class="fa-regular fa-file-video"></i> ${textoBase}</span>`;
            } else {
                let num = item.linkIndex;
                let episodeNumber = num < 10 ? ` 0${num}` : ` ${num}`;
                episodeTitleHtml = `<span class="episode-title"><i class="fa-regular fa-file-video"></i> ${textoBase}${episodeNumber}</span>`;
            }

            tabContentsHtml += `<li><a href="${item.linkUrl}" target="_blank" rel="noopener noreferrer" class="item-link" style="${backgroundStyle}">` +
                               `<span class="server-title"><i class="fa-regular fa-circle-play"></i> ${item.serverName}</span>` +
                               episodeTitleHtml +
                               `<span class="action-button"><i class="fa-solid fa-up-right-from-square"></i> Ver Ahora</span>` +
                               `</a></li>`;
        });
        tabContentsHtml += '</ul></div>';
        firstTabActive = false;
    }

    servers.forEach(server => {
        if (!processedServers.includes(server.name)) {
            const serverName = server.name.toUpperCase();
            const linksContent = server.content;
            const links = linksContent.split(/<br\s*\/?>|\n/).map(link => link.trim()).filter(link => link !== '');

            if (links.length > 0) {
                anyTabFound = true;
                const isActive = firstTabActive ? ' active' : '';
                const tabId = server.name.toLowerCase().replace(/\s/g, '');

                tabButtonsHtml += `<button class="tab-button${isActive}" data-tab="${tabId}"><i class="fa-regular fa-circle-play"></i> ${serverName}</button>`;
                tabContentsHtml += `<div id="tab-${tabId}" class="tab-content${isActive}"><ul>`;

                const backgroundStyle = backgroundUrl ? `background-image: url('${backgroundUrl}');` : '';

                links.forEach((link, index) => {
                    let episodeTitleHtml = '';
                    if (tipoContenido.toLowerCase() === 'av') {
                        const num = index + 1;
                        const episodeNumber = num < 10 ? ` 0${num}` : ` ${num}`;
                        episodeTitleHtml = `<span class="episode-title"><i class="fa-regular fa-file-video"></i> ${textoBase}${episodeNumber}</span>`;
                    } else if (tipoContenido.toLowerCase() === 'pelicula') {
                        episodeTitleHtml = `<span class="episode-title"><i class="fa-regular fa-file-video"></i> ${textoBase}</span>`;
                    } else {
                        let episodeNumber = '';
                        if (tieneNumero) {
                            const num = index + 1;
                            episodeNumber = num < 10 ? ` 0${num}` : ` ${num}`;
                        }
                        episodeTitleHtml = `<span class="episode-title"><i class="fa-regular fa-file-video"></i> ${textoBase}${episodeNumber}</span>`;
                    }

                    tabContentsHtml += `<li><a href="${link}" target="_blank" rel="noopener noreferrer" class="item-link" style="${backgroundStyle}">` +
                                       `<span class="server-title"><i class="fa-regular fa-circle-play"></i> ${serverName}</span>` +
                                       episodeTitleHtml +
                                       `<span class="action-button"><i class="fa-solid fa-up-right-from-square"></i> Ver Ahora</span>` +
                                       `</a></li>`;
                });

                tabContentsHtml += '</ul></div>';
                firstTabActive = false;
                finalContent = finalContent.replace(server.fullMatch, '');
            }
        }
    });

    const downloadRegex = /<Carpetas-Descarga>\s*([\s\S]*?)\s*<\/Carpetas-Descarga>/gi;
    const downloadMatch = downloadRegex.exec(rawContent);

    if (downloadMatch) {
        anyTabFound = true;
        const downloadContent = downloadMatch[1];
        const isActive = firstTabActive ? ' active' : '';
        const tabId = 'descarga';

        tabButtonsHtml += `<button class="tab-button${isActive}" data-tab="${tabId}"><i class="fa-solid fa-cloud-arrow-down"></i> DESCARGAR</button>`;
        tabContentsHtml += `<div id="tab-${tabId}" class="tab-content${isActive}"><ul class="download-list">`;

        const carpetaRegex = /<Carpeta>\s*([^|]+?)\s*\|\|\s*([^<]+?)\s*<\/Carpeta>/gi;
        let carpetaMatch;

        while ((carpetaMatch = carpetaRegex.exec(downloadContent)) !== null) {
            const nameAndLink = carpetaMatch[1].trim();
            const linkUrl = carpetaMatch[2].trim();
            const backgroundStyle = backgroundUrl ? `background-image: url('${backgroundUrl}');` : '';

            tabContentsHtml += `<li><a href="${linkUrl}" target="_blank" rel="noopener noreferrer" class="item-link" style="${backgroundStyle}"><div class="download-link-info"><span class="download-name">${nameAndLink}</span></div><span class="action-button"><i class="fa-regular fa-folder"></i> Ir al Enlace</span></a></li>`;
        }

        tabContentsHtml += '</ul></div>';
        firstTabActive = false;
        finalContent = finalContent.replace(downloadMatch[0], '');
    }

    tabButtonsHtml += '</div>';

    if (anyTabFound) {
        tabsContainer.innerHTML = tabButtonsHtml + tabContentsHtml;
        sourceData.innerHTML = finalContent.trim();
        tabsContainer.classList.add('visible');

        if (sourceData.innerHTML.trim() === '') {
            sourceData.style.display = 'none';
        } else {
            sourceData.style.display = 'block';
        }

        const tabButtons = tabsContainer.querySelectorAll('.tab-button');
        tabButtons.forEach(button => {
            button.addEventListener('click', () => {
                tabButtons.forEach(btn => btn.classList.remove('active'));
                tabsContainer.querySelectorAll('.tab-content').forEach(content => content.classList.remove('active'));
                button.classList.add('active');
                const targetTabId = button.getAttribute('data-tab');
                const targetTab = document.getElementById(`tab-${targetTabId}`);
                if (targetTab) {
                    targetTab.classList.add('active');
                }
            });
        });

    } else {
        tabsContainer.style.display = 'none';
        sourceData.style.display = 'block';
    }
});
//]]]]><![CDATA[>
